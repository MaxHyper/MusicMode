{
	"manifest_version": 2,
	"name": "Music Mode for YouTube",
	"short_name": "Music Mode",
	"description": "AutoSkip to the actual song or video on YouTube!",
	"version": "0.1",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [{
		"js": ["bg.js"],
		"matches": ["*://*.youtube.com/*"],
    "run_at": "document_start"
	}],
	"permissions": [
		"activeTab",
		"tabs",
		"storage",
		"webNavigation"
	]
}
