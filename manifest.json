{
  "manifest_version": 2,

  "name": "Skip2Actual",
  "description": "Skip to actual video",
  "version": "0.2",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "js": ["bg.js"],
    "matches": ["https://*/*"],   //["*://www.youtu/be.com*"]
    "run_at": "document_start",
    "all_frames": true
  }],
  "permissions": [
    "activeTab",
    "contextMenus",
    "tabs","storage","webNavigation"
  ]

}
